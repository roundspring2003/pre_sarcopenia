<Window x:Class="pre_sarcopenia.sln.Camera"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pre_sarcopenia.sln"
        mc:Ignorable="d"
        Title="Camera" Height="450" Width="700">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="550*"/>
            <RowDefinition Height="150*"/>
        </Grid.RowDefinitions>
        <Image Name="image" HorizontalAlignment="Left" Height="244" Margin="105,0,0,52" VerticalAlignment="Bottom" Width="482"/>
        <TextBox Name="box" HorizontalAlignment="Center" Grid.Row="1" TextWrapping="Wrap"  VerticalAlignment="Center" Width="120" RenderTransformOrigin="-0.171,0.299"/>
    </Grid>
</Window>
